{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "align": [true, "parameters", "statements"],
        "forin": false,
        "max-classes-per-file": false,
        "no-var-requires": false,
        "no-shadowed-variable": false,
        "no-unused-expression": false,
        "no-console": false,
        "variable-name": false
    },
    "rulesDirectory": []
}
